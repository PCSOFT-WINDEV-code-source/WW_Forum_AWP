  procedures :
   -
     name : Connecte
     internal_properties : CAAAAAgAAACs91BklxzGtVrzkSAlcI9q2uzpneX2uXWFg96myBJlYZm39XAe9pIB4s3Z7MhLaGu8/RG753EzvzNbWIWW/vyvFqCmRjlmI/X+nZO+jBjTYHMIUGMKlHhBzvA2I+ORY4ERHGAsiRFjO0sD1RRnC8yZnQhMmcY69K2Yqk4yhxXFO7a8
     procedure_id : 1412719167200182401
     type_code : 14
     code : |1-
      
      // Résumé : Lance la procédure de connexion
      //
      PROCEDURE Connecte(sLogin est une chaîne, sPassword est une chaine)
      
      bSurUtilisateur est un booléen = faux
      
      // Recherche de l'utilisateur
      HLitRecherche(Utilisateur,pseudo,sLogin,hIdentique)
      Si htrouve(Utilisateur) ALORS
      	bSurUtilisateur = vrai
      SINON
      	HLitRecherche(Utilisateur,AdresseEmail,sLogin,hIdentique)
      	Si htrouve(Utilisateur) ALORS
      		bSurUtilisateur = Vrai
      	FIN
      FIN
      
      // Vérification du mot de passe
      SI bSurUtilisateur ALORS
      	si Utilisateur.MotDePasse = CrypteMDP(sPassword) ALORS
      		Utilisateur.DateDernièreVisite = DateSys() + heuresys()
      		HModifie(Utilisateur)
      		gnIDUtilisateurEnCours = Utilisateur.IDUtilisateur
      		renvoyer "1"
      	FIN
      FIn
      	
      renvoyer "0"
     type : 458752
   -
     name : MotDePassePerdu
     procedure_id : 1412719167200248046
     type_code : 14
     code : |1+
      
      // Résumé : Envoie son mot de de passe à l'utilisateur
      //
      PROCEDURE MotDePassePerdu()
      
      bSurUtilisateur est un booléen = faux
      
      sEmail est une chaine = [
      	Cher %1,
      	
      	Vous nous avez indiqué que vous avez oublié votre mot de passe.
      	
      	Le voici : %2
      ]
      
      MaSession est un EmailSessionSMTP = SessionSMTP()
      MonMessage est un Email
      
      SI EmailOuvreSession(MaSession) ALORS
      	// Session ouverte
      	MonMessage.AdresseExpéditeur = ChargePref(EMAIL_EXPEDITEUR)
      	MonMessage.Expéditeur = ChargePref(EMAIL_EXPEDITEUR)
      	Ajoute(MonMessage.Destinataire,Utilisateur.AdresseEmail)
      	MonMessage.Sujet = "Mot de passe oublié"
      	MonMessage.Message = ChaîneConstruit(sEmail,Utilisateur.Pseudo,DécrypteMDP(Utilisateur.MotDePasse))
      	
      	Si EmailEnvoieMessage(MaSession,MonMessage) ALORS
      		// Message envoyé
      		LIB_Erreur..Visible  = Faux
      		retour	
      	SINON
      		// Erreur lors de l'envoi du message"
      		erreur(ErreurInfo())
      	FIN
      	
      	EmailFermeSession(MaSession)
      SINON
      	// Impossible d'ouvrir une session avec le serveur SMTP
      	Erreur(ErreurInfo())
      FIN
      
     type : 458752
