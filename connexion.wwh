        p_codes :
         -
           code : |1-
            
            si SAI_Login = "" ALORS
            	LIB_Erreur = "Veuillez entrer un nom d'utilisateur ou une adresse e-mail."
            	LIB_Erreur..Visible = vrai
            	Anime(SAI_Login..Alias,animations.shake)
            	RepriseSaisie(SAI_Login)
            
            FIN
            
            SI SAI_Password = "" ALORS
            	LIB_Erreur = "Veuillez entrer un mot de passe."
            	LIB_Erreur..Visible = Vrai
            	Anime(SAI_Password..Alias,animations.shake)
            	RepriseSaisie(SAI_Password)
            FIN
            
            
            // Connexion
            sIDUtilisateur est une chaine = AJAXExécute(Connecte,SAI_Login,SAI_Password)
            
            SI sIDUtilisateur <> "0" ALORS
            	PageAffiche(accueil)
            SINON
            	SAI_Password = ""
            	LIB_Erreur = "Utilisateur ou mot de passe incorrect"
            	LIB_Erreur..Visible = Vrai
            	Anime(CELL_Login..Alias,animations.shake)
            FIN
